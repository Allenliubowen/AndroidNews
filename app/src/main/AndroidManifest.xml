<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhjohow.androidnews">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name="com.zhjohow.androidnews.app.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.zhjohow.androidnews.ui.news.activity.PhotosDetailActivity"
            android:theme="@style/BaseAppTheme.Trans"
            />
        <activity android:name="com.zhjohow.androidnews.ui.main.activity.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name="com.zhjohow.androidnews.ui.news.activity.NewsDetailActivity"/>
        <activity android:name="com.zhjohow.androidnews.ui.news.activity.NewsBrowserActivity"/>
        <activity android:name="com.zhjohow.androidnews.ui.news.activity.NewsPhotoDetailActivity"/>
        <activity android:name="com.zhjohow.androidnews.ui.news.activity.NewsChannelActivity"/>

        <!-- 友盟统计 -->
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="" >
        </meta-data>
    </application>

</manifest>
